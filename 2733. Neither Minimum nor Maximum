class Solution:
    def findNonMinOrMax(self, nums: List[int]) -> int:
        nums.sort()
        '''min_value = nums[0]
        max_value = nums[len(nums)-1]
        for i in nums:
            if i > min_value and i < max_value:
                return i
        return -1'''
        if len(nums) > 2:
            return nums[1]
        else:
            return -1

        
